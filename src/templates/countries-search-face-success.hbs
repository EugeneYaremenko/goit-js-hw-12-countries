<h1>{{name}}</h1>
    <ul>
        <li><span>Capital:</span> {{capital}}</li>
        <li><span>Population:</span> {{population}}</li>
        <li>
            <span>Languages:</span>
            <ul>
                {{#each languages}}
                <li>{{this.name}}</li>
                {{/each}}
            </ul>
        </li>
    </ul>

    <img src="{{flag}}" alt="Flag of {{name}}">